<form action="/songs/new" method="post">
  <label for="song_name">Name</label>
  <input id="song_name" type="text" name="song[name]" value="">
  <label for="artist_name">Artist Name</label>
  <input id="artist_name" type="text" name="artist[name]" value="">
  <% @genres.each do |genre| %>
    <label for="<%= genre.name %>"><%= genre.name %></label>
    <input id="<%= genre.name %>" type="checkbox" name="genres[]" value="<%= genre.name %>">
  <% end %>
  <input type="submit" name="submit" value="Create">
</form>
